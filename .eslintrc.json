{
	"env": {
		"browser": false,
		"es6": true,
		"node": true
	},
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint", "jest"],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"no-unused-vars": ["off"],
		"@typescript-eslint/no-explicit-any": "off"
	},
	"parserOptions": {
		"sourceType": "module"
	}
}
